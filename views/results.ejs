<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Search Results</title>
</head>

<body>
  <h1>Search Results</h1>
  <a href="/">Back to Search</a>
  <ul>
    <% results.forEach(result=> { %>
      <li>
        <% Object.entries(result).forEach(([key, value])=> { %>
          <div><strong>
              <%= key %>:
            </strong>
            <%= value %>
          </div>
          <% }) %>
      </li>
      <% }) %>
  </ul>
</body>

</html>